{
  "availables": {
    "createReactComponent": {
      "arguments": {
        "name": {
          "type": "string",
          "desc": "Component name",
          "required": true
        },
        "target_dir": {
          "type": "string",
          "desc": "Target directory",
          "required": true
        }
      }
    },
    "createModelEntity": {
      "arguments": {
        "table_name": {
          "type": "string",
          "desc": "Table name",
          "required": true
        },
        "target_dir": {
          "type": "string",
          "desc": "Target directory, for individual output must set to custom then specify --out-model-dir and --out-entity-dir",
          "required": true
        },
        "--schema": {
          "type": "string",
          "desc": "Json schema Model entity definition",
          "required": false,
          "defaultValue": null
        },
        "--out-model-dir": {
          "type": "string",
          "desc": "Target Model directory to save with",
          "required": false,
          "defaultValue": null
        },
        "--out-entity-dir": {
          "type": "string",
          "desc": "Target Entity directory to save with",
          "required": false,
          "defaultValue": null
        }
      }
    },
    "createModelAPIRoute": {
      "arguments": {
        "table_name": {
          "type": "string",
          "desc": "Table name",
          "required": true
        },
        "target_dir": {
          "type": "string",
          "desc": "Target directory",
          "required": true
        },
        "json_path": {
          "type": "string",
          "desc": "Model entity definition",
          "required": false,
          "defaultValue": "./model_entities.json"
        }
      }
    },
    "createModelAPIRouteV2": {
      "arguments": {
        "table_name": {
          "type": "string",
          "desc": "Table name",
          "required": true
        },
        "target_dir": {
          "type": "string",
          "desc": "Target directory",
          "required": true
        },
        "json_path": {
          "type": "string",
          "desc": "Model entity definition",
          "required": false,
          "defaultValue": "./model_entities.json"
        }
      }
    },
    "listModelAPIRoutes": {
      "arguments": {
        "json_path": {
          "type": "string",
          "desc": "Model entity definition",
          "required": false,
          "defaultValue": "./model_entities.json"
        }
      }
    },
    "createModuleAction": {
      "arguments": {
        "actionName": {
          "type": "string",
          "desc": "Action name",
          "required": true
        },
        "arguments": {
          "type": "string",
          "desc": "Argument list separated by comma",
          "required": true
        },
        "json_path": {
          "type": "string",
          "desc": "Actions definition",
          "required": false,
          "defaultValue": "./lib-artisan/actions.json"
        }
      }
    },
    "sideMenu": {
      "arguments": {
        "list|add|rm": {
          "type": "string",
          "required": 0,
          "desc": ""
        },
        "menu_key": {
          "type": "string",
          "required": 0,
          "desc": ""
        },
        "title": {
          "type": "string",
          "required": 0,
          "desc": ""
        },
        "path": {
          "type": "string",
          "required": 0,
          "desc": ""
        },
        "icon": {
          "type": "string",
          "required": 0,
          "desc": ""
        },
        "--json_path=side_menu.json": {
          "type": "string",
          "required": false,
          "desc": "kwargs",
          "defaultValue": "side_menu.json"
        }
      }
    },
    "createSchemaDef": {
      "arguments": {
        "table_name": {
          "type": "string",
          "required": true,
          "desc": "Table name to be stored on the schema definition json"
        },
        "pk": {
          "type": "string",
          "required": true,
          "desc": "Primary key of the table"
        },
        "columns": {
          "type": "string",
          "required": true,
          "desc": "Column field list must be in array format [field1,field2,field3]"
        },
        "types": {
          "type": "string",
          "required": true,
          "desc": "Column field type must be in array format based on field index [int,varchar,datetime]"
        },
        "nullables": {
          "type": "string",
          "required": false,
          "desc": "Column field list wich nullable must be in array format [field1,field2]"
        },
        "lengths": {
          "type": "number",
          "required": false,
          "desc": "Column field length must be in array format based on field index [null,20,null]"
        },
        "validations": {
          "type": "string",
          "required": false,
          "desc": "Column field validation method to be created as validation model [[required,min=8],null,null]"
        },
        "--schema=model_entities.json": {
          "type": "string",
          "required": false,
          "desc": "flags",
          "defaultValue": "./model_entities.json"
        }
      }
    },
    "html2React": {
      "arguments": {
        "input": {
          "type": "string",
          "required": true,
          "desc": "Input file html"
        },
        "output": {
          "type": "string",
          "required": true,
          "desc": "Output File jsx"
        }
      }
    },
    "createModelFormComponent": {
      "arguments": {
        "table_name": {
          "type": "string",
          "required": true,
          "desc": ""
        },
        "target_dir": {
          "type": "string",
          "required": true,
          "desc": ""
        },
        "json_path": {
          "type": "string",
          "required": true,
          "desc": ""
        }
      }
    },
    "createModelGridComponent": {
      "arguments": {
        "table_name": {
          "type": "string",
          "required": true,
          "desc": ""
        },
        "target_dir": {
          "type": "string",
          "required": true,
          "desc": ""
        },
        "json_path": {
          "type": "string",
          "required": true,
          "desc": ""
        }
      }
    }
  }
}